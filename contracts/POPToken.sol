// File: POPToken.sol

contract POPToken is Context, ERC20, Ownable {
    // No one can edit these after deployment
    mapping(address => bool) private _isExcluded;
    mapping(address => bool) private _isExcludedFromMaxTx;
    mapping(address => bool) private _isExcludedFromMaxWallet;

    // IMPORTANT: CHANGE THIS TO MAINNET BEFORE DEPLOYMENT
    address public constant UNISWAPV2ROUTER =
        0x4752ba5DBc23f44D87826276BF6Fd6b1C372aD24;
    IUniswapV2Router02 public immutable uniswapV2Router;
    address public immutable uniswapV2Pair;

    string public constant _name = "Proof of Prescence (POP)";
    string public constant _symbol = "POP";
    uint8 private constant _decimals = 18;

    // Tokenomics
    uint256 private _totalSupply = 1_000_000 * 10 ** 18; // 1 million tokens with 18 decimals
    uint16 public buyFee = 990; // 0.99%
    uint16 public sellFee = 990; // 0.99%
    uint16 public burnFee = 10; // 0.10%
    uint256 public feeDenominator = 100000; // 100%
    uint256 public maxTxAmount = (_totalSupply * 2000) / feeDenominator;
    uint256 public maxWallet = (_totalSupply * 2000) / feeDenominator;
    uint256 public blockToLiftMaxWallet;

    uint256 private swapThreshold = (_totalSupply * 100) / feeDenominator;
    uint256 public maxAmountPerSwap = (_totalSupply * 100) / feeDenominator; // 0.1% of total supply
    uint256 public rewardThreshold = 1 ether;
    address payable public rewardWallet;

    bool public feeEnabled = true;
    bool public inSwap = false;
    bool public swapForRewardEnabled = true;
    bool public tradingEnabled = false;

    // Rewards Distribution
    uint64 public maxRewardWinnersPerRound = 1000; // SEND 10 PERCENT to ADMIN WALLET
    uint64 public rewardRound = 0;
    uint64 public remainingWinnersForCurrentRound = maxRewardWinnersPerRound;

    address public constant DEAD = 0x000000000000000000000000000000000000dEaD;
    address private constant ZERO = 0x0000000000000000000000000000000000000000;
    address public adminWallet;

    event SwappedEthForReward(uint256 tokensSwapped, uint256 ethReceived);
    event RewardThresholdReached(uint256 contractBalance);
    event RewardDistributed(uint64 round, uint256 ethAmount);
    event SwapForRewardEnabled(bool status);

    modifier lockTheSwap() {
        inSwap = true;
        _;
        inSwap = false;
    }

    modifier onlyAdmin() {
        require(_msgSender() == adminWallet, "Caller is not the admin");
        _;
    }

    constructor(
        address _owner,
        address _adminWallet,
        address[] memory _initAddresses
    ) ERC20(_name, _symbol) Ownable(_owner) {
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
            UNISWAPV2ROUTER
        );
        // Create a uniswap pair for this new token
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), _uniswapV2Router.WETH());

        // set the rest of the contract variables
        uniswapV2Router = _uniswapV2Router;

        _mint(_owner, _totalSupply);
        _approve(address(this), UNISWAPV2ROUTER, type(uint256).max);
        _approve(address(this), address(uniswapV2Pair), type(uint256).max);
        _approve(address(this), _owner, type(uint256).max);

        adminWallet = _adminWallet;
        rewardWallet = payable(_owner);

        _isExcluded[adminWallet] = true;
        _isExcluded[_owner] = true;
        _isExcludedFromMaxTx[_owner] = true;
        _isExcludedFromMaxWallet[_owner] = true;
        _isExcludedFromMaxTx[adminWallet] = true;
        _isExcludedFromMaxTx[address(this)] = true;
        _isExcludedFromMaxTx[DEAD] = true;
        _isExcludedFromMaxTx[ZERO] = true;
        _isExcludedFromMaxWallet[adminWallet] = true;
        _isExcludedFromMaxWallet[address(this)] = true;
        _isExcludedFromMaxWallet[DEAD] = true;
        _isExcludedFromMaxWallet[ZERO] = true;

        for (uint256 i = 0; i < _initAddresses.length; i++) {
            _isExcluded[_initAddresses[i]] = true;
        }

        emit Transfer(address(0), msg.sender, _totalSupply);
    }

    /**
     * @dev Returns the name of the token.
     */
    function name() public pure override returns (string memory) {
        return _name;
    }

    /**
     * @dev Returns the symbol of the token, usually a shorter version of the
     * name.
     */
    function symbol() public pure override returns (string memory) {
        return _symbol;
    }

    /**
     * @dev Returns the number of decimals used to get its user representation.
     */
    function decimals() public pure override returns (uint8) {
        return uint8(_decimals);
    }

    /**
     * @dev See {IERC20-totalSupply}.
     */
    function totalSupply() public view virtual override returns (uint256) {
        return _totalSupply;
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        if (!_isExcluded[from] && !_isExcluded[to]) {
            require(tradingEnabled, "Trading is not enabled");
            if (block.number > blockToLiftMaxWallet) {
                maxWallet = 0;
                maxTxAmount = 0;
            }
        } else {
            super._transfer(from, to, amount);
            return;
        }
        if (maxTxAmount > 0) {
            if (!_isExcludedFromMaxTx[from] && !_isExcludedFromMaxTx[to]) {
                require(
                    amount <= maxTxAmount,
                    "Transfer amount exceeds the maxTxAmount."
                );
            }
        }
        if (maxWallet > 0) {
            if (
                (!_isExcludedFromMaxWallet[from] &&
                    !_isExcludedFromMaxWallet[to])
            ) {
                if (to != uniswapV2Pair) {
                    require(
                        balanceOf(to) + amount <= maxWallet,
                        "Transfer amount exceeds the maxWallet."
                    );
                }
            }
        }

        // Check if we need to swap for rewards
        checkAndMakeSwap(from);

        if ((from != uniswapV2Pair && to != uniswapV2Pair)) {
            super._transfer(from, to, amount);
        } else {
            _transferWithFee(from, to, amount);
        }
    }

    function checkAndMakeSwap(address from) private {
        uint256 contractTokenBalance = balanceOf(address(this));
        if (contractTokenBalance >= swapThreshold) {
            if (swapForRewardEnabled && !inSwap && from != uniswapV2Pair) {
                // Swap entire contract balance for rewards
                uint256 swapAmount = contractTokenBalance > maxAmountPerSwap
                    ? maxAmountPerSwap
                    : contractTokenBalance;
                swapTokensForEth(swapAmount);
                emit SwappedEthForReward(
                    contractTokenBalance,
                    address(this).balance
                );
            }
        }
        if (address(this).balance >= rewardThreshold) {
            emit RewardThresholdReached(address(this).balance);
        }
    }

    function _transferWithFee(
        address from,
        address to,
        uint256 amount
    ) internal {
        uint256 burnAmount = (amount * burnFee) / feeDenominator;

        // if buy transaction
        if (from == uniswapV2Pair) {
            uint256 buyFeeAmount = (amount * buyFee) / feeDenominator;
            amount = amount - buyFeeAmount - burnAmount;
            if (buyFeeAmount > 0) {
                super._transfer(from, address(this), buyFeeAmount);
            }
        } else {
            // if sell transaction
            uint256 sellFeeAmount = (amount * sellFee) / feeDenominator;
            amount = amount - sellFeeAmount - burnAmount;
            if (sellFeeAmount > 0) {
                super._transfer(from, address(this), sellFeeAmount);
            }
        }

        if (burnAmount > 0) {
            super._transfer(from, DEAD, burnAmount);
        }
        // Transfer the entire amount to the to
        super._transfer(from, to, amount);
    }

    function swapTokensForEth(uint256 tokenAmount) private lockTheSwap {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), UNISWAPV2ROUTER, tokenAmount);

        // make the swap
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );

        // Send 20% rewards to the reward wallet
        uint256 rewardAmount = (address(this).balance * 20) / 100;
        (bool success, ) = payable(rewardWallet).call{value: rewardAmount}("");
        require(success, "Transfer of ETH to Reward Wallet failed");

        // Send 10% rewards to the admin wallet
        uint256 adminAmount = (address(this).balance * 10) / 100;
        (success, ) = payable(adminWallet).call{value: adminAmount}("");
        require(success, "Transfer of ETH to Admin Wallet failed");
    }

    /**
     * @notice Distributes rewards to tos.
     * @param tos The addresses to receive rewards.
     * @param rewards The corresponding reward amounts (in wei), precomputed off-chain.
     * @dev The contract must have sufficient balance to distribute rewards.
     */
    function distributeRewards(
        address[] calldata tos,
        uint256[] calldata rewards
    ) external onlyAdmin {
        require(
            tos.length == rewards.length,
            "tos and rewards arrays must have the same length"
        );
        require(
            tos.length <= maxRewardWinnersPerRound,
            "Batch exceeds maxRewardWinnersPerRound"
        );

        uint256 totalDistributed = 0;
        for (uint256 i = 0; i < tos.length; i++) {
            require(
                tos[i] != address(0) || tos[i] != DEAD,
                "Invalid to address"
            );
            require(rewards[i] > 0, "Invalid reward amount");

            // Use call instead of transfer for gas savings and support for contracts
            (bool success, ) = tos[i].call{value: rewards[i]}("");
            require(success, "Transfer failed");

            totalDistributed += rewards[i];
        }

        // Update state for reward rounds
        if (tos.length >= remainingWinnersForCurrentRound) {
            rewardRound++;
            remainingWinnersForCurrentRound = maxRewardWinnersPerRound;
            emit RewardDistributed(rewardRound, totalDistributed);
        } else {
            remainingWinnersForCurrentRound -= uint64(tos.length);
        }
    }

    function enableTrading() external onlyOwner {
        tradingEnabled = true;
        blockToLiftMaxWallet = block.number + 14400;
    }

    function setAdminWallet(address _adminWallet) external onlyOwner {
        adminWallet = _adminWallet;
    }

    //to receive ETH from uniswapV2Router when swapping
    receive() external payable {}
}
